{
    "name": "Bible Memory Verse Tab",
    "version": "1.0",
    "description": "Chrome extension that overrides the New Tab page and helps the user memorize Bible verses",
    "permissions": [
        "activeTab", 
        "declarativeContent", 
        "storage"
    ],
    "background": {
      "scripts": ["jquery.min.js", "script.js"],
      "persistent": false
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "manifest_version": 2,
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    }
}
